<module rename-to="eureka">
    <inherits name='com.google.gwt.user.User' />
    <inherits name="com.google.gwt.json.JSON" />
    <inherits name="com.google.gwt.resources.Resources" />
    <inherits name="org.eurekastreams.commons.ActionRPCService" />
    <inherits name="org.eurekastreams.server.Domain" />
    <inherits name='com.allen_sauer.gwt.dnd.gwt-dnd' />
    <inherits name='net.sf.gilead.Gilead4Gwt'/>
    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name='com.googlecode.gchart.GChart'/>
    <inherits name='com.google.gwt.widgetideas.GWTCanvas' />
    
    <set-property name="user.agent" value="gecko1_8"/>    
    <set-configuration-property name="CssResource.style" value="pretty"/>
	<set-property name="gwt.logging.popupHandler" value="DISABLED" />

    <entry-point class='org.eurekastreams.web.client.ui.pages.master.ApplicationEntryPoint' />
    <entry-point class='org.eurekastreams.web.client.ui.pages.master.ConnectEntryPoint' />

    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.ChangeListenerCollection"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.ClickListenerCollection"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.FocusListenerCollection"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.FormHandlerCollection"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.KeyboardListenerCollection"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.LoadListenerCollection"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.MouseListenerCollection"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.MouseWheelListenerCollection"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.PopupListenerCollection"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.ScrollListenerCollection"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.TabListenerCollection"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.TableListenerCollection"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.TreeListenerCollection"/>
    <extend-configuration-property name="rpc.blacklist" value="com.allen_sauer.gwt.dnd.client.DragHandlerCollection"/>
    <extend-configuration-property name="rpc.blacklist" value="com.allen_sauer.gwt.dnd.client.VetoDragException"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.canvas.*"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.dom.client.*"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.cellview.client.HasKeyboardPagingPolicy"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.cellview.client.HasKeyboardSelectionPolicy"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.cellview.client.SimplePager"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.DockLayoutPanel"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.MultiWordSuggestOracle"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.PopupPanel"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.SuggestOracle"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.ValueBoxBase"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.view.client.DefaultSelectionEventManager"/>
    <extend-configuration-property name="rpc.blacklist" value="org.eurekastreams.server.domain.OAuthConsumer"/>
    <extend-configuration-property name="rpc.blacklist" value="org.eurekastreams.server.domain.Person"/>
    <extend-configuration-property name="rpc.blacklist" value="org.eurekastreams.server.domain.AppData"/>
    <extend-configuration-property name="rpc.blacklist" value="org.eurekastreams.server.domain.AppDataValue"/>
    <extend-configuration-property name="rpc.blacklist" value="com.google.gwt.user.client.ui.*"/>
    <extend-configuration-property name="rpc.blacklist" value="org.apache.shindig.*"/>
    <extend-configuration-property name="rpc.blacklist" value="org.eurekastreams.commons.actions.context.ActionContext"/>
    <extend-configuration-property name="rpc.blacklist" value="org.eurekastreams.server.domain.strategies.BookmarkStreamFilterAdapter"/>
</module>
